{
  "name": "hyperswitch",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "web": "webpack serve --mode=development --config webpack.config.js",
    "web:next": "next",
    "ios": "rm -rf ios/build && react-native run-ios",
    "android": "react-native run-android && yarn run adb",
    "build:web": "rm -rf dist/ && webpack --mode=production --config webpack.config.js && mv -f dist/index.bundle.js ~/Desktop/juspay/rescript-euler-dashboard/public/common/Hyperswitch",
    "build:lib": "cd android && ./gradlew build && ./gradlew publish && cd ../node_modules/react-native-gradle-plugin && ./gradlew build && ./gradlew publish && cp -rf maven/com ../../android/maven",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/hyperswitch.bundle && cp android/app/src/main/assets/hyperswitch.bundle @juspay-tech/react-native-hyperswitch/android/src/main/assets",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/hyperswitch/hyperWrapper/Resources/hyperswitch.bundle && cp ios/hyperswitch/hyperWrapper/Resources/hyperswitch.bundle @juspay-tech/react-native-hyperswitch/ios/Resources",
    "codepush:android": "appcenter codepush release-react -b hyperswitch.bundle -a Hyperswitch/hyperswitch-android -d Production",
    "codepush:ios": "appcenter codepush release-react -b hyperswitch.bundle -a Hyperswitch/hyperswitch-ios -d Production",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && rm -rf yarn.lock && yarn run clean:gradle && yarn run clean:pod",
    "re:build": "rescript",
    "re:clean": "rescript clean",
    "re:start": "rescript build -w",
    "postinstall": "patch-package",
    "pod": "cd ios && pod install && cd ..",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "clean:gradle": "cd android && ./gradlew clean && cd ..",
    "clean:pod": "cd ios && pod deintegrate && cd ..",
    "clean:gradle:cache": "rm -rf ~/.gradle",
    "clean:pod:cache": "pod cache clean --all",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@sentry/react-native": "^5.9.1",
    "react-native-code-push": "^8.0.1",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-pager-view": "^6.1.2",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.20.5",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/metro-config": "^0.72.11",
    "@react-navigation/material-top-tabs": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@rescript/react": "^0.10.3",
    "@sentry/nextjs": "^7.73.0",
    "@svgr/webpack": "^6.5.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.2.21",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-native-web": "^0.18.12",
    "bs-fetch": "^0.6.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-next": "^13.5.5",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "next": "^13.5.4",
    "patch-package": "^6.5.1",
    "prettier": "^2.4.1",
    "react": "18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.5",
    "react-native-codegen": "0.71.5",
    "react-native-tab-view": "^3.5.1",
    "react-native-web": "^0.18.12",
    "react-native-web-linear-gradient": "^1.1.2",
    "react-native-web-lottie": "^1.4.4",
    "react-test-renderer": "18.2.0",
    "rescript": "^10.1.4",
    "rescript-react-native": "^0.70.0",
    "rescript-react-navigation": "^6.0.3",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.2.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "jest": {
    "preset": "react-native"
  }  
}
